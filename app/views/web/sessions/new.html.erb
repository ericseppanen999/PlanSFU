# app/views/web/sessions/new.html.erb
<h1>Log in</h1>
<%= form_with url: login_path, local: true do |form| %>
    <div>
        <%= form.label :username %>
        <%= form.text_field :username, name: 'user[username]' %>
    </div>

    <div>
        <%= form.label :password %>
        <%= form.password_field :password, name: 'user[password]' %>
    </div>

    <div>
        <%= form.submit "Sign in" %>
    </div>
<% end %>